@model JobOffer.Models.Addressh

@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_DashboardAdminLayout.cshtml";
}

<div class="page-header" style="margin-left:250px;margin-top:30px">
    <div class="page-block">
        <div class="row align-items-center">
            <div class="col-md-12">
                <div class="page-header-title">
                    <h5 class="m-b-10" style="font-size:20px;font-weight:bold; color:#89ba16">Manage Address</h5>
                </div>
                <ul class="breadcrumb">
                    <li class="breadcrumb-item"><a asp-action="Dashboard" asp-controller="Admin"><i class="feather icon-home"></i></a></li>
                    <li class="breadcrumb-item"><a href="#!">Manage Address</a></li>
                    <li class="breadcrumb-item"><a href="#!">Edit Adress</a></li>

                </ul>
            </div>
        </div>
    </div>
</div>


<div style="width:1000px;margin-left:250px">
    <div class="col-md-4">
        <form asp-action="EditAddress">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Addressid" />
            <div class="form-group">
                <label asp-for="Addressname" class="control-label"></label>
                <input asp-for="Addressname" class="form-control" />
                <span asp-validation-for="Addressname" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Addresscity" class="control-label"></label>
                <input asp-for="Addresscity" class="form-control" />
                <span asp-validation-for="Addresscity" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" /> |
                <a asp-action="MainAddress">Back to List</a>
            </div>
        </form>
    </div>


</div>

